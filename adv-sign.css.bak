input.input-signup-txt-name, input.input-signup-txt, input.input-pwd-signup, input.input-pwd-signup-2, select.industry-type, input.input-signin-txt, 
input.input-pwd-signin, input.inputs1, input.input-pwd-signin-2, input.fee, 
input.subscription-person-fname, input.subscription-person-lname {
  width: 100%;
  padding: 15px;
  margin: 5px 0 22px 0;
  display: inline-block;
  border: none;
  background: #f1f1f1;
}

/* Add a background color when the inputs get focus */
input.input-signup-txt-name:focus, input.input-signup-txt:focus, input.input-pwd-signup:focus, input.input-pwd-signup-2:focus, 
select.industry-type:focus, input.input-signin-txt:focus, input.input-pwd-signin:focus, input.inputs1:focus,
input.input-pwd-signin-2:focus, input.fee:focus, 
input.subscription-person-fname:focus, input.subscription-person-lname:focus {
  background-color: #ddd;
  outline: none;
}

select.industry-type{
  cursor: pointer;
}

/* Set a style for all buttons */
button.cancelbtn, button.signupbtn ,button.signinbtn, button.inputs1{
  background-color: #04AA6D;
  color: white;
  padding: 14px 20px;
  margin: 8px 0;
  border: none;
  cursor: pointer;
  width: 100%;
  opacity: 0.9;
}

button.cancelbtn:hover, button.signupbtn:hover ,button.signinbtn:hover
, button.inputs1:hover {
  opacity:1;
}

/* Extra styles for the cancel button */
button.cancelbtn {
  padding: 14px 20px;
  background-color: rgba(10, 13, 20, 1);/*#f44336;*/
}

button.signinbtn, button.signupbtn{
	background-color: rgba(10, 13, 20, 1);
}

/* Float cancel and signup buttons and add an equal width */
button.cancelbtn, button.signupbtn ,button.signinbtn{
  float: left;
  width: 50%;
}

button.inputs1, button.inputs1:focus {
  width: 100%;
  background-color: #04AA6D;
  border: none;
  color: white;
  font-size: 20px;
}

/* Add padding to container elements */
.container-signup, .container-signin, .container-subscribe-3, 
.container-payment-services {
  padding: 16px;
  background: black;
}

/* The Modal (background) */
.modal-signup, .modal-signin, .modal-subscribe-3, .modal-coursery-playground {
  display: none; /* Hidden by default */
  position: fixed; /* Stay in place */
  z-index: 1; /* Sit on top */
  left: 0;
  top: 0;
  width: 100%; /* Full width */
  height: 100%; /* Full height */
  overflow: auto; /* Enable scroll if needed */
  background-color: rgb(0,0,0); /* Fallback color */
  background-color: rgba(0,0,0,0.9); /* Black w/ opacity */
  padding-top: 50px;
}

/* Modal Content/Box */
.modal-content-signup, .modal-content-signin, .modal-content-subscribe-3, 
.modal-coursery-container, .modal-coursery-playground {
  background-color: #fefefe;
  margin: 5% auto 15% auto; /* 5% from the top, 15% from the bottom and centered */
  border: 1px solid #888;
  width: 80%; /* Could be more or less, depending on screen size */
}

.modal-coursery-container, .modal-coursery-playground{
	width: 100%;
	margin: 0;
}

.modal-coursery-playground{
	display: none;
	z-index: 4;
	background-color: rgba(5, 5, 5, 1);
	border: none;
	padding-top: 0px;
	overflow-x: hidden;
}

.modal-content-signup, .modal-content-signin, .modal-coursery-container, .modal-coursery-playground {
  -webkit-animation-name: zoom;
  -webkit-animation-duration: 0.6s;
  animation-name: zoom;
  animation-duration: 0.6s;
}

.modal-content-subscribe-3{
  z-index: 28;	
}

/* Style the horizontal ruler */
hr {
  border: 1px solid #f1f1f1;
  margin-bottom: 25px;
}
 
/* The Close Button (x) */
.close-signup, .close-signin {
  position: absolute;
  right: 35px;
  top: 15px;
  font-size: 40px;
  font-weight: bold;
  color: #f1f1f1;
}

.close-signup:hover, .close-signin:hover,
.close-signup:focus, .close-signup:focus {
  color: #f44336;
  cursor: pointer;
}

/* Clear floats */
.clearfix::after {
  content: "";
  clear: both;
  display: table;
}
.container-signup input, .container-signin input{font-size: 20px;height: 40px;}
