/* Full-width input fields */
label, .terms-conditions, .please-fill {color: white;}
.signup-header{
       -moz-animation-name: didcx-welcome;
	   -webkit-animation-name: didcx-welcome;
	   animation-name: signup-header;
	   animation-iteration-count: infinite;
	   animation-timing-function: ease-in-out;
	   animation-delay: 2s;
	   animation-duration: 2s;
	   background-color: #000;	
	   font-family: Arial, Helvetica, sans-serif;
}
   @keyframes signup-header{
      from{color: red;}
	  to{color: yellow;}
   }
.gaga-relate{
       -moz-animation-name: didcx-welcome;
	   -webkit-animation-name: didcx-welcome;
	   animation-name: gaga-relate;
	   animation-iteration-count: infinite;
	   animation-timing-function: ease-in-out;
	   animation-delay: 2s;
	   animation-duration: 2s;
	   background-color: #000;	
	   font-family: Arial, Helvetica, sans-serif;
	   color: red;
}
   @keyframes gaga-relate{
      from{color: white;}
	  to{color: white;}
      /*from{color: red;}
	  to{color: yellow;}*/
   }
.gaga-relate-2 {
  width: 200px;
  
  background-color: black;
  position: relative;
  animation-name: gaga-relate-2;
  animation-duration: 4s;
  animation-direction: reverse; 
  animation-iteration-count: infinite; 
  /* padding: 35px; */
  font-family: Arial, Helvetica, sans-serif;
  animation-timing-function: ease-in-out;
  font-size: 17px;
}
.gaga-relate-3 {
  width: 200px;
  
  background-color: black;
  position: relative;
  animation-name: gaga-relate-3;
  animation-duration: 4s;
  animation-direction: reverse; 
  animation-iteration-count: infinite; 
  /* padding: 35px; */
  text-align: center;
  font-family: Arial, Helvetica, sans-serif;
  animation-timing-function: ease-in-out;
  font-size: 17px;
}

@keyframes gaga-relate-2 {
  0%   {background-color:red; left:0px; top:0px;}
  25%  {background-color:yellow; left:70%; top:0px;}
  50%  {background-color:blue; left:70%; top:70px;}
  75%  {background-color:green; left:0px; top: 70px;}
  100% {background-color:red; left:0px; top:0px;}
}
@keyframes gaga-relate-3 {
  0%   {background-color:red; left:5%; top:0px;}
  25%  {background-color:yellow; left:81%; top:0px;}
  50%  {background-color:blue; left:81%; top:0px;}
  75%  {background-color:green; left:5%; top: 0px;}
  100% {background-color:red; left:5%; top:0px;}
}
